<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Venture Real Estate</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/styles.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/owl.theme.default.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js" defer></script>
    <script src="~/Scripts/owl.carousel.min.js" defer></script>
    <script src="~/script.js"></script>
</head>
<body>
    <div id="sidenav" class="sidenav">
        <div class="text-center">
            <a href="/"><i class="fas fa-arrow-left"></i> Back to main page</a>
        </div>
        <div class="text-center py-3">
            @Html.ActionLink("Listing", "index", "ListingManage", null, new { @class = "sidenavLink" })
            @Html.ActionLink("Customer", "index", "Customers", null, new { @class = "sidenavLink" })
            @Html.ActionLink("Agent", "index", "Agents", null, new { @class = "sidenavLink" })
        </div>
    </div>
    <div class="main">
        @RenderBody()
    </div>
    <script src="https://kit.fontawesome.com/bf5dc9b95c.js" crossorigin="anonymous"></script>
</body>
</html>
